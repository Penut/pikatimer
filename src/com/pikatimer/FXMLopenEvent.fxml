<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="rootGridPane" alignment="CENTER" hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pikatimer.FXMLopenEventController">
    <children>
      <HBox GridPane.rowIndex="1">
         <children>
            <Button mnemonicParsing="false" text="Clone Race" textAlignment="CENTER" GridPane.rowIndex="1">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button mnemonicParsing="false" onAction="#openEvent" text="Open" GridPane.rowIndex="1">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button mnemonicParsing="false" onAction="#newEvent" text="New Race" textAlignment="CENTER" GridPane.rowIndex="1">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
         </children>
      </HBox>
      <VBox>
         <children>
            <Label alignment="CENTER" prefHeight="43.0" prefWidth="193.0" text="PikaTimer" GridPane.columnIndex="1" />
            <Label fx:id="eventName" alignment="CENTER" prefHeight="43.0" prefWidth="193.0" text="Race File Name" />
         </children>
      </VBox>
   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="95.0" minHeight="17.0" prefHeight="43.0" />
      <RowConstraints maxHeight="112.0" minHeight="34.0" prefHeight="86.0" />
   </rowConstraints>
</GridPane>
