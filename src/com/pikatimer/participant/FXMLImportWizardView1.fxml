<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>


<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="fileNameLabel" prefHeight="35.0" prefWidth="233.0" text="Choose a file... ">
                <font>
                    <Font size="24.0" />
                </font>
            </Label>
            <Button fx:id="fileChooserButton" alignment="CENTER" mnemonicParsing="false" text="Choose File" textAlignment="CENTER" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
      <CheckBox mnemonicParsing="false" text="Merge / Update" />
      <CheckBox mnemonicParsing="false" text="Cleanup City / State" />
      <CheckBox mnemonicParsing="false" text="Cleanup Names" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
