   <children>
      <HBox spacing="5.0">
         <children>
            <TextField fx:id="resultsSearchTextField" promptText="Search" />
            <ChoiceBox fx:id="raceChoiceBox" prefWidth="150.0" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" />
   </padding>
